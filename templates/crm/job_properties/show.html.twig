{% extends 'base.html.twig' %}

{% block title %}JobProperties{% endblock %}

{% block body %}
    <h1>JobProperties</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ job_property.id }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ job_property.created ? job_property.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ job_property.updated ? job_property.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ job_property.name }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ job_property.value }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('crm_job_properties_index') }}">back to list</a>

    <a href="{{ path('crm_job_properties_edit', {'id': job_property.id}) }}">edit</a>

