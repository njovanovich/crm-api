{% extends 'base.html.twig' %}

{% block title %}Service{% endblock %}

{% block body %}
    <h1>Service</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ service.id }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ service.created ? service.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ service.updated ? service.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ServiceNumber</th>
                <td>{{ service.serviceNumber }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ service.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('crm_service_index') }}">back to list</a>

    <a href="{{ path('crm_service_edit', {'id': service.id}) }}">edit</a>

    {{ include('crm_service/_delete_form.html.twig') }}
{% endblock %}
