{% extends 'base.html.twig' %}

{% block title %}AdminProperties{% endblock %}

{% block body %}
    <h1>AdminProperties</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ admin_property.id }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ admin_property.created ? admin_property.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ admin_property.updated ? admin_property.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ admin_property.name }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ admin_property.value }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('crm_admin_properties_index') }}">back to list</a>

    <a href="{{ path('crm_admin_properties_edit', {'id': admin_property.id}) }}">edit</a>

    {{ include('crm_admin_properties/_delete_form.html.twig') }}
